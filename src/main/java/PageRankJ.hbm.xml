<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="mgr.PageRankJ" table="pagerank">
   		<id name="eidJ" type="int">
            <column name="eid" precision="10" not-null="true" />
        </id>
        <property name="mailJ" type="text">
            <column name="mail" length="31" not-null="false" />
        </property>
    </class>
            
 </hibernate-mapping>  