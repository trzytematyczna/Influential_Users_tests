<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">  
  
 <hibernate-mapping>  
  <class name="sql.ReferenceInfoJ" table="ReferenceInfo">
   <id name="rfidJ" type="int">
            <column name="rfid" precision="10" not-null="true" />
        </id>
        <property name="midJ" type="int">
            <column name="mid" length="10" not-null="true" />
        </property>
        <property name="referenceJ" type="text">
            <column name="reference" length="20" not-null="false" />
        </property>
    </class>
            
 </hibernate-mapping>  